{
  "name": "Solid Rewind",
  "description": "SolidJS timetravel debugger",
  "version": "1.0",
  "manifest_version": 3,
  "devtools_page": "devtools.html",
  "action": {
    "default_popup": "index.html",
    "default_title": "Popup"
  },
  "background": {
    "service_worker" : "background.js"
  },
  "content_scripts": [
    {
      // "matches": ["http://*/*", "https://*/*"],
      "matches": ["<all_urls>"],
      "js": ["context-script.js"]
    }
  ],
  // "web_accessible_resources" : ["inject-script.js"],
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "clipboardWrite",
    "clipboardRead"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}